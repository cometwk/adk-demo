{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
    "skipLibCheck": true,

    "paths": {
      // "@example/dev": ["../dev/src/index"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
